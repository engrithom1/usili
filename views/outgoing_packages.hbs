                    pp += "<br><br>"
                    pp += "Sender details<br>"
                    pp += "Sender name :" + response.data.sender_name + "<br>"
                    pp += "Sender phone :" + response.data.sender_phone + "<br>"
                    pp += "Receiver name :" + response.data.receiver_name + "<br>"
                    pp += "Receiver phone :" + response.data.receiver_phone
                    pp += "<br><br>"
                    pp += "Package name :" + response.data.name + "<br>"
                    pp += "Package weight :" + response.data.package_weight + "kg<br>"
                    pp += "Package value :TSH " + response.data.package_value + ".00<br>"
                    pp += "Package size :" + response.data.package_size + "<br>"
                    pp += "Package type :" + response.data.package_type
                    pp += "<br><br>"
                    pp += "Agent name :" + response.data.fulname + "<br>"
                    pp += "Agent phone :" + response.data.phone1 + "<br>"
                    pp += "Bus company :Usiri Transportation Services<br>"
                    pp += "Company phone :0768448525<br>"
                    pp += "CUSTOMER SERVICES :0768448525<br>"
                    pp += "" + response.data.bb_from_region + " :" + response.data.bb_from_contact + "<br>"
                    pp += "" + response.data.bb_to_region + " :" + response.data.bb_to_contact + "<br>"

                    $('#pp').html(pp)
                    $('#codedata-img').attr('src', response.data.code_data)

                    $('#receiptModal').modal('show')

                    

                } else {
                    alert(response.msg)
                }

            })
            .catch((error) => {
                console.log(error)
                alert('Check internet, try again')
            });

    })

